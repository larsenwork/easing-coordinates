{
  "name": "easing-coordinates",
  "version": "1.0.4",
  "description":
    "Utility script that takes an easing function as input and outputs a coordinate set with adjustable precision/resolution.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larsenwork/easing-coordinates.git"
  },
  "files": ["dist"],
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "pretty": "pretty-quick",
    "es6-build": "tsc",
    "es6-watch": "tsc --watch",
    "cjs-build": "tsc --module commonjs --declaration false --outDir dist/cjs",
    "cjs-watch": "tsc --watch --module commonjs --declaration false --outDir dist/cjs",
    "build": "npm run clean && npm run es6-build & npm run cjs-build; npm run pretty",
    "watch": "npm run es6-watch & npm run cjs-watch",
    "test": "jest",
    "test-ci":
      "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "jest --coverage"
  },
  "keywords": ["easing-functions", "javascript"],
  "author": "larsenwork <andreas@larsenwork.com> (https://larsenwork.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larsenwork/easing-coordinates/issues"
  },
  "dependencies": {
    "@types/bezier-js": "0.0.7",
    "bezier-js": "^2.2.5"
  },
  "homepage": "https://larsenwork.com/easing-gradients/",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "coveralls": "^3.0.1",
    "husky": "^1.0.0-rc.7",
    "jest": "^23.0.0",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
